{
  "name": "Sample",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Sample Chrome Extension",
  "browser_action": {
    "default_popup": "chromeMenu/chromeMenu.html"
  },
  "icons": {
    "16": "chromeMenu/icon.png"
  },
  "background": {
    "scripts":[
      "background/background.js"
    ],
    "persistent": false
  },
  "content_security_policy":
    "script-src 'self' https://code.jquery.com/jquery-3.0.0.min.js; object-src 'self'"
  
  
}